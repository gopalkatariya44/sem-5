<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <Button
        android:id="@+id/explicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="activity 2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/view_implicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="Open Google"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/explicit" />


    <Button
        android:id="@+id/dial_implicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="Dial"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_implicit" />

    <Button
        android:id="@+id/call_implicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="Call"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dial_implicit" />

    <Button
        android:id="@+id/send_implicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="Send"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/call_implicit" />

    <Button
        android:id="@+id/send_to_implicit"
        android:layout_width="230dp"
        android:layout_height="52dp"
        android:layout_marginTop="45dp"
        android:text="Send TO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/send_implicit" />

</androidx.constraintlayout.widget.ConstraintLayout>